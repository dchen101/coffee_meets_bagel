<%= @user.username %>
<%= form_for @photo, url: user_photos_path(@user), html:{multipart: true} do |f| %>
  <%= f.file_field :image %>
  <%= f.submit "Upload Image" %>
<% end %>

<ul>
  <% @user.photos.each do |photo| %>
    <li><%= link_to image_tag( photo.image.url(:medium)), user_photo_path(@user, photo) %>
      <% if photo.liked?(@user.id) %>
        <%= button_to "Dislike", dislike_path(photo), method: 'delete' %>
      <% else %>
        <%= button_to "Like", like_path(photo)%>
      <% end %>
    </li>
  <%end%>
</ul>
