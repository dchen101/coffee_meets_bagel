<h1>Coffee Meets Bagel</h1>
<%= form_tag('/sessions') do  %>
  <%= text_field_tag "username", nil, placeholder: "username"%>
  <%= password_field_tag "password", nil, placeholder: "password" %>
  <%=submit_tag "Sign In"%>
<% end %>

<%= form_tag('/users') do  %>
  <%= text_field_tag "username", nil, placeholder: "username"%>
  <%= password_field_tag "password", nil, placeholder: "password" %>
  <%=submit_tag "Sign Up"%>
<% end %>

